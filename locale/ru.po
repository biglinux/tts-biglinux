msgstr "Нет доступных голосов"

msgid "No TTS voices installed"
msgstr "Голоса TTS не установлены"

msgid "No voices available for this engine"
msgstr "Нет доступных голосов для этого движка"

#, python-brace-format
msgid "Install {engine} voices first"
msgstr "Сначала установите голоса {engine}"

#, python-brace-format
msgid "{count} voices available"
msgstr "{count} голосов доступно"

